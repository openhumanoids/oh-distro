package drc;

// Direct duplicate of ROS's
// sensor_msgs/PointCloud2 
// In particular the data block
// should cast to a pcl point cloud
// added by mfallon.

struct pointcloud2_t
{
    int64_t utime;

    int32_t seq;
    string frame_id;
    int32_t height;
    int32_t width;

    int32_t nfields;
    drc.pointfield_t fields[nfields];
    
    boolean is_bigendian;
    int32_t point_step;
    int32_t row_step;
    int32_t data_nbytes;
    byte data[data_nbytes];
    boolean is_dense;
}
