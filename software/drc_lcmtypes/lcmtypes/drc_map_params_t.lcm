package drc;

struct map_params_t
{
  // timestamp of this message
  int64_t utime;

  // id of map to create
  int64_t map_id;

  // resolution in m
  float resolution;

  // xyz bounds in m
  float bound_min[3];
  float bound_max[3];

  // number of scans to store in circular buffer
  int32_t buffer_size;
}
