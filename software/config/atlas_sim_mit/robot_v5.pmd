script "-2.run_box" {
    start cmd "base_deputy";
    wait ms 1000;
    start group "0.params_and_model_pub";
    wait ms 1000;
    start group "1.state_mit";
    start cmd "CombinedPlanner";
    start group "5.maps";
    start cmd "drakeAtlasSimulation Box";
    start cmd "PlanEvalBothHands";
    start cmd "InstQPBothHands";
    start cmd "director";
}

