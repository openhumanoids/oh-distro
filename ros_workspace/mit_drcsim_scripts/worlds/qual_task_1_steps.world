        <collision name="right_collision">
          <pose>1.4 3.1 0 0 0 0</pose>
          <geometry>
            <box>
              <size>3 0.2 1.0</size>
            </box>
          </geometry>
        </collision>
        <visual name="right_visual">
          <pose>1.4 3.1 0 0 0 0</pose>
          <geometry>
            <box>
              <size>3 0.2 1.0</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/drc_qual.material</uri>
              <name>DRCQual/WarningWall</name>
            </script>
          </material>
        </visual>

      </link>
    </model>

    <!-- Start: Gate 0 -->
    <include>
      <name>gate_0</name>
      <uri>model://gate</uri>
      <pose>1 0 3.0 0 0 0</pose>
    </include>

